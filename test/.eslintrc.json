{
    "rules": {
        "indent": [
            1,
            4
        ],
        "quotes": [
            1,
            "single"
        ],
        "linebreak-style": [
            2,
            "windows"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-console": 1,        
        "no-unused-vars": [2, {"vars": "local", "argsIgnorePattern": "^(_|([a-z]$))"}],        
        "camelcase":[2, {"properties": "always"}],
        "curly":2,
        "no-implied-eval":2        
    }, 
    "globals": {
                "jQuery": false,
                "$": true
    },
    "env": {        
        "node": true,
        "amd": true
    },
    "ecmaFeatures": {
        "modules": true,
        "classes": true
    },
    "extends": "eslint:recommended"    
}
